<!-- dotted_outline.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <stroke
        android:color="@color/Dark"
        android:width="2dp"
        android:dashWidth="4dp"
        android:dashGap="4dp" />
    <solid android:color="@color/OffWhite" />
    <corners android:radius="8dp" />
</shape>
